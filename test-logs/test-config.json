{
  "test_suites": {
    "cisco_ios": {
      "description": "Cisco IOS device log parsing tests",
      "test_files": [
        "cisco-ios/bgp/bgp_neighbor_down.log",
        "cisco-ios/interface/interface_flap.log",
        "cisco-ios/ospf/ospf_neighbor_changes.log",
        "cisco-ios/system/config_changes.log"
      ],
      "expected_patterns": [
        "BGP-5-ADJCHANGE",
        "LINEPROTO-5-UPDOWN",
        "OSPF-5-ADJCHG",
        "SYS-5-CONFIG_I"
      ]
    },
    "cisco_asa": {
      "description": "Cisco ASA firewall log parsing tests",
      "test_files": [
        "cisco-asa/security/firewall_blocks.log",
        "cisco-asa/security/real_asa_samples.log"
      ],
      "expected_patterns": [
        "ASA-4-106023",
        "ASA-6-302013",
        "ASA-6-302014"
      ]
    },
    "cisco_nexus": {
      "description": "Cisco Nexus NX-OS log parsing tests",
      "test_files": [
        "cisco-nx-os/interface/nexus_port_status.log"
      ],
      "expected_patterns": [
        "ETHPORT-5-IF_DOWN",
        "ETHPORT-5-IF_UP"
      ]
    }
  },
  "test_scenarios": {
    "bgp_troubleshooting": {
      "description": "Test BGP session troubleshooting scenarios",
      "test_files": ["cisco-ios/bgp/bgp_neighbor_down.log"],
      "ai_queries": [
        "Why is BGP neighbor 192.168.1.1 down?",
        "What caused the BGP session to terminate?",
        "How to fix BGP neighbor relationship?"
      ]
    },
    "interface_flapping": {
      "description": "Test interface flapping detection",
      "test_files": ["cisco-ios/interface/interface_flap.log"],
      "ai_queries": [
        "Why is GigabitEthernet0/1 flapping?",
        "How to troubleshoot interface instability?"
      ]
    },
    "security_analysis": {
      "description": "Test security event analysis",
      "test_files": ["cisco-asa/security/firewall_blocks.log"],
      "ai_queries": [
        "What security threats were detected?",
        "Are there any suspicious connection attempts?"
      ]
    }
  }
}